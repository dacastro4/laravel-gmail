{
  "name": "ddomanskyi/laravel-gmail",
  "description": "Gmail API package for Laravel from Dacastro4 (https://github.com/dacastro4/laravel-gmail/) with multiaccounts handling improvement",
  "keywords": [
    "api",
    "laravel",
    "Gmail"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Denys Domanskyi",
      "email": "domanskyidenys@gmail.com",
      "homepage": "http://div-art.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "google/apiclient": "^2.4",
    "illuminate/auth": "~5.8|^6.0",
    "illuminate/config": "~5.8|^6.0",
    "illuminate/database": "~5.8|^6.0",
    "illuminate/routing": "~5.8|^6.0",
    "illuminate/session": "~5.8|^6.0",
    "illuminate/support": "~5.8|^6.0",
    "swiftmailer/swiftmailer": "~5.8|^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.0",
    "squizlabs/php_codesniffer": "~3.4",
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Ddomanskyi\\LaravelGmail\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ddomanskyi\\LaravelGmail\\LaravelGmailServiceProvider"
      ],
      "aliases": {
        "LaravelGmail": "Ddomanskyi\\LaravelGmail\\Facade\\LaravelGmail"
      }
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
