{
    "name": "dacastro4/laravel-gmail",
    "description": "Gmail API package for Laravel",
    "keywords": [
        "api",
        "laravel",
        "Gmail"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Castro",
            "email": "danielcastro04@gmail.com",
            "homepage": "https://danielcastro.dev"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "google/apiclient": "^v2.12.6",
        "illuminate/auth": "~5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/config": "~5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "~5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/routing": "~5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/session": "~5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "~5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "symfony/mime": "^5.4|^6.0|^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3|^10.5",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "~3.4",
        "orchestra/testbench": "^v4.18.0|^v5.20.0|^v6.25.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dacastro4\\LaravelGmail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dacastro4\\LaravelGmail\\LaravelGmailServiceProvider"
            ],
            "aliases": {
                "LaravelGmail": "Dacastro4\\LaravelGmail\\Facade\\LaravelGmail"
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
